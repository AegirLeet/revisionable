{
    "name": "aegirleet/revisionable",
    "license": "MIT",
    "description": "Keep a revision history for your models without thinking, created as a package for use with Laravel",
    "keywords": [
        "model",
        "laravel",
        "ardent",
        "revision",
        "history"
    ],
    "homepage": "http://github.com/AegirLeet/revisionable",
    "authors": [
        {
            "name": "Chris Duell",
            "email": "me@chrisduell.com",
            "role": "Original author"
        },
        {
            "name": "AegirLeet",
            "email": "aegir@aegir.sexy",
            "role": "Current maintainer"
        }
    ],
    "support": {
        "issues": "https://github.com/AegirLeet/revisionable/issues",
        "source": "https://github.com/AegirLeet/revisionable"
    },
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "~5.0"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-4": {
            "AegirLeet\\Revisionable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AegirLeet\\Revisionable\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "roave/security-advisories": "dev-master"
    }
}
